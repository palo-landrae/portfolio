generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model markdowns {
  id        String @id @default(dbgenerated()) @map("_id") @db.ObjectId
  contents  String
  file_name String
}

model projects {
  id          String @id @default(dbgenerated()) @map("_id") @db.ObjectId
  color       String
  demo        String
  description String
  git_repo    String
  title       String
}
